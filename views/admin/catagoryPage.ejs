<div class="row">
    <table class="table col-md-6">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Catagory</th>
            </tr>
        </thead>
        <tbody>
            <%catagories.forEach((item,index)=>{ %>
            <tr>
                <th scope="row"><%=index+1%> </th>
                <td id="in"><%=item.name %></td>
            </tr>
            <% })  %>
        </tbody>
    </table>
    <form action="/admin/add-catagory" method="post">
    <form class="ml-5">
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Catagory
                Name</label>
            <input type="text" name="catagory" class="form-control" id="exampleInputEmail1"
                aria-describedby="emailHelp">

        </div>
        <button type="submit" class="btn btn-primary">Add</button>
    </form>
</div>
</form>