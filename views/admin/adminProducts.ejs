<!-- <button class="bg-primary text-danger"><a href=" product</a></button> -->
<div class="adbtn"><a class="btn btn-primary" href="/admin/products/add" role="button">Add Products</a></div>
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Image</th>
        <th scope="col">Name</th>
        <th scope="col">price</th>
        <th scope="col">action</th>
      </tr>
    </thead>
    <tbody>
      <% products.forEach((product,index)=>{ %>
        <tr>
          <th scope="row"><%= index+1 %> </th>
          <td><img width="50px" height="50px" src="/img/14promax.jpg" alt=""></td>
          <td><%= product.name %> </td>
          <td><%= product.price %></td>
          <td>
            <a class="btn btn-primary" href="" role="button">Edit</a>
            <a class="btn btn-primary"name="deletebtn " id="deletebtn"   href="" role="button" onclick="deleteProduct('<%= product.product %>')">Delete</a>
          </td>
        </tr>
      <% })  %>
    </tbody>
  </table>